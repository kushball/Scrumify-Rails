<h1>Listing data_field_steps</h1>

<table>
  <tr>
    <th>Data field</th>
    <th>Body</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @data_field_steps.each do |data_field_step| %>
  <tr>
    <td><%= data_field_step.data_field %></td>
    <td><%= data_field_step.body %></td>
    <td><%= link_to 'Show', data_field_step %></td>
    <td><%= link_to 'Edit', edit_data_field_step_path(data_field_step) %></td>
    <td><%= link_to 'Destroy', data_field_step, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

