<h1>Listing documentations</h1>

<table>
  <tr>
    <th>Group</th>
    <th>User</th>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @documentations.each do |documentation| %>
  <tr>
    <td><%= documentation.group %></td>
    <td><%= documentation.user %></td>
    <td><%= documentation.name %></td>
    <td><%= link_to 'Show', documentation %></td>
    <td><%= link_to 'Edit', edit_documentation_path(documentation) %></td>
    <td><%= link_to 'Destroy', documentation, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Documentation', new_documentation_path %>
