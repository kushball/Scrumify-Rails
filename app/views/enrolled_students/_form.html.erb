<%= form_for @enrolled_student do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :roster_id %><br />
    <%= f.text_field :roster_id %>
  </p>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p><%= f.submit %></p>
<% end %>
